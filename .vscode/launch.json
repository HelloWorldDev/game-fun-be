{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Single Goroutine",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "PORT": "4882",
                "APP_ENV": "test",
                "kafka": "1"
            },
            "buildFlags": ["-gcflags=all=-N -l"],
            "dlvFlags": ["--check-go-version=false"],
            "trace": "verbose",
            "showLog": true,
            "args": [],
            "hideSystemGoroutines": true,
            "showRegisters": true,
            "logOutput": "debugger"
        }
    ]
}